{
  "manifest_version": 2,

  "name": "Recreation.gov Reserver",
  "description": "Auto-reserve campsites at recreation.gov",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html",
    "default_title": "Recreation.gov Reserver"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
